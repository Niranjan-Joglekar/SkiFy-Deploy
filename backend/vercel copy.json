{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/test_eg/(.*)",
        "dest": "/main.py"
      },
      {
        "src": "/resume/(.*)",
        "dest": "/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "/main.py"
      }
    ]
  }
  

